{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "semi": [2, "always"],
    "max-len": [2, 120, 2],
    "no-continue": "off",
    "no-console": "off",
    "no-plusplus": "off",
    "no-minusminus": "off"
  }
}
